    <script>
        const app = angular.module('app', ['angularUtils.directives.dirPagination'])
        app.controller('AnnouncementController', ['$scope', '$http', function($scope, $http){

            $scope.announcement = [];
            $scope.totalUsers = 0;
            $scope.currentPage = 1;
            $scope.limit = 10;
            getResultsPage(1);

            $scope.pagination = {
                current: 1
            };

            $scope.pageChanged = function(newPage) {
                getResultsPage(newPage);
            };

            $('#announcementSearchBtn').on('click', function(e){
                getResultsPage(1);
            });

            function getResultsPage(pageNumber) {
                var dateFrom  = $('#announcementDateFromInput').val();

                if (dateFrom !== '') {
                    dateFrom = moment(dateFrom, 'DD-MM-YYYY').format('YYYYMMDD');
                }

                var dateTo  = $('#announcementDateToInput').val();

                if (dateTo !== '') {
                    dateTo = moment(dateTo, 'DD-MM-YYYY').format('YYYYMMDD');
                }

                var emitenCode = getParams('kodeEmiten');
                var keyword = $('#announcementKeywordInput').val();
                $http.get("@Url.Action("GetProfileAnnouncement", "ListedCompany")" +
                '?kodeEmiten=' + emitenCode +
                '&keyword=' + keyword  +
                '&dateFrom=' + dateFrom +
                '&dateTo=' + dateTo +
                '&indexFrom=' +(pageNumber-1) +
                '&pageSize=' + $scope.limit
                + '&language=' + currentLang)
                .then(function (result) {
                    $scope.announcement = result.data.Replies;
                    $scope.totalUsers = result.data.ResultCount;
                });
            }

        }])
    </script>
    <script>
        function getEmitenType() {
            $.get('@Url.Action("GetEmiten", "Helper")?emitenType=*', function (result) {
                $('#emitenList').append('<option value="">' + "@Umbraco.GetDictionaryValue("choose emiten")" + '</option>');
                for (var i = 0; i < result.length; i++) {
                    $('#emitenList').each(function(){
                        $(this).append('<option value="' + result[i].KodeEmiten + '">' + result[i].KodeEmiten + ' - ' + result[i].NamaEmiten + '</option>');
                    });
                }
            });

            $('#emitenList').on('change', function (e) {
                var url = document.URL.replace('kodeEmiten=' + getParams('kodeEmiten'), 'kodeEmiten=' + $(this).val());
                window.location.href = url;
            });
        }
        function getCompanyProfile(api) {
            var emitenCode = getParams('kodeEmiten');

            var emitenType = $('input[name="companyProfileType"]:checked').val();

            if (emitenType === undefined) {
                emitenType = '';
            }

            var indexFrom = 1;
            var pageSize = 1;

            $.get(api + '?emitenType = ' + emitenType + '&kodeEmiten=' +
                emitenCode + '&language=' + currentLang, function (result) {
                    if (result.Profiles !== null && result.Profiles.length > 0) {
                        setCompanyProfiles(result.Profiles[0]);
                    }

                    if (result.Sekretaris !== null && result.Sekretaris.length > 0) {
                        setCompanySecretary(result.Sekretaris);
                    }

                    if (result.Direktur !== null && result.Direktur.length > 0) {
                        setDirector(result.Direktur);
                    }

                    if (result.Komisaris !== null && result.Komisaris.length > 0) {
                        setComissioner(result.Komisaris);
                    }

                    if (result.KomiteAudit !== null && result.KomiteAudit.length > 0) {
                        setAuditCommittee(result.KomiteAudit);
                    }

                    if (result.PemegangSaham !== null && result.PemegangSaham.length > 0) {
                        setShareholders(result.PemegangSaham);
                    }

                    if (result.AnakPerusahaan !== null && result.AnakPerusahaan.length > 0) {
                        setSubsidiary(result.AnakPerusahaan);
                    }

                    if (result.Dividen !== null ) {
                        if(result.Dividen.length > 0){
                            setDividen(result.Dividen);
                        }
                    }

                    if (result.BondsAndSukuk !== null ) {
                        if(result.BondsAndSukuk.length > 0){
                            setBondsAndSukuk(result.BondsAndSukuk);
                        }
                    }

                    if (result.IssuedBond !== null ) {
                        if(result.IssuedBond.length > 0){
                            console.log(result.IssuedBond);
                            setIssuedBond(result.IssuedBond);
                        }
                    }

                    if (result.KAP !== null ) {
                        if(result.KAP.length > 0){
                            setKAP(result.KAP);
                        }
                    } else {
                        $('#public-accountant-container').hide();
                    }
                });
        }

