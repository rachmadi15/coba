            $('#emitenLogo').attr('src', profiles.Logo);
            $('#emitenName').text(profiles.NamaEmiten === '' || profiles.NamaEmiten === 'NULL' ? '-' : profiles.NamaEmiten);
            $('#frName').text(profiles.NamaEmiten === '' || profiles.NamaEmiten === 'NULL' ? '-' : profiles.NamaEmiten);
            $('#frSecurityCode').text(profiles.KodeEmiten === '' || profiles.KodeEmiten === 'NULL' ? '-' : profiles.KodeEmiten);
